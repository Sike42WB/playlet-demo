{
    "endpoints": [
        {
            "name": "trending",
            "displayName": "Trending",
            "url": "/api/v1/trending",
            "authenticated": false,
            "queryParams": {
                "type": {
                    "required": false,
                    "type": [
                        "Music",
                        "Gaming",
                        "Movies"
                    ]
                },
                "region": {
                    "required": false,
                    "type": "#ISO3166",
                    "default": "GetUserCountryCode"
                }
            }
        },
        {
            "name": "popular",
            "displayName": "Popular",
            "url": "/api/v1/popular",
            "authenticated": false
        },
        {
            "name": "search",
            "displayName": "Search",
            "url": "/api/v1/search",
            "queryParams": {
                "q": {
                    "required": true,
                    "type": "string"
                },
                "sort_by": {
                    "required": false,
                    "type": [
                        "relevance",
                        "rating",
                        "upload_date",
                        "view_count"
                    ]
                },
                "date": {
                    "required": false,
                    "type": [
                        "hour",
                        "today",
                        "week",
                        "month",
                        "year"
                    ]
                },
                "duration": {
                    "required": false,
                    "type": [
                        "short",
                        "long"
                    ]
                },
                "type": {
                    "required": false,
                    "type": [
                        "video",
                        "playlist",
                        "channel",
                        "all"
                    ]
                },
                "features": {
                    "required": false,
                    "type": [
                        "hd",
                        "subtitles",
                        "creative_commons",
                        "3d",
                        "live",
                        "purchased",
                        "4k",
                        "360",
                        "location",
                        "hdr"
                    ]
                },
                "region": {
                    "required": false,
                    "type": "#ISO3166",
                    "default": "GetUserCountryCode"
                }
            }
        },
        {
            "name": "auth_feed",
            "displayName": "User Feed",
            "url": "/api/v1/auth/feed",
            "authenticated": true,
            "requestArgs": {
                "retryCount": 3
            },
            "responseHandler": "AuthFeedHandler"
        },
        {
            "name": "playlists",
            "displayName": "Playlists",
            "url": "/api/v1/auth/playlists",
            "authenticated": true,
            "requestArgs": {
                "retryCount": 3
            },
            "responseHandler": "AuthPlaylistsHandler"
        }
    ]
}
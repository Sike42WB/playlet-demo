<?xml version="1.0" encoding="UTF-8"?>

<component name="VideoPlayer" extends="Video">
    <interface>
        <field id="sender" type="node" />
        <field id="chapter" type="string" alias="chapterLabel.text" />
    </interface>
    <children>
        <Label
            id="chapterLabel"
            width="350"
            height="25"
            horizAlign="center"
            vertAlign="center"
            font="font:SmallestSystemFont"
        />
        <Timer
            id="chapterLabelTimer"
            repeat="true"
            duration="0.25"
        />
    </children>
</component>
import "pkg:/source/services/Invidious.bs"

function Init()
    m.top.functionName = "TaskMain"
end function

function TaskMain()
    input = m.top.getField("input")
    videoId = input.videoId

    metadata = Invidious.GetVideoMetadata(videoId)

    m.top.setField("output", {
        videoId: videoId,
        metadata: metadata
    })
end function


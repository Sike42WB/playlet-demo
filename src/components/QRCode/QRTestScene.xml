<?xml version='1.0' encoding='utf-8'?>
<component
		name='QRTestScene'
		extends='Scene'>
	<children>
		<!-- This is example use of QRPoster: show QRCode centered on screen -->
		<QRPoster
				id='TestQRPoster'
				loadWidth='500'
				loadHeight='500'
				loadPadding='20'
				translation='[100, 110]' />
		<Label
				id='instructionsTitle'
				height='50'
				width='500'
				text='Instructions'
				font='font:MediumBoldSystemFont'
				wrap='true'
				translation='[650,110]' />
		<Label
				id='instructions'
				height='500'
				width='500'
				font='font:MediumSystemFont'
				wrap='true'
				translation='[650,160]' />
	</children>

</component>
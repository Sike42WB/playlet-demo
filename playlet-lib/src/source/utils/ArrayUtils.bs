import "pkg:/source/utils/Types.bs"

namespace ArrayUtils

    function IndexOfString(array as object, value as dynamic) as integer
        if not IsString(value)
            return -1
        end if
        for i = 0 to array.Count() - 1
            if array[i] = value
                return i
            end if
        end for
        return -1
    end function

end namespace

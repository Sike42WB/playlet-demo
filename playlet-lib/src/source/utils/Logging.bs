function LogArgToString(value as dynamic) as string
    valueType = type(value)

    if valueType = "<uninitialized>"
        return valueType
    else if value = invalid
        return "<invalid>"
    else if GetInterface(value, "ifToStr") <> invalid
        return value.toStr()
    else if valueType = "roSGNode"
        return "Node(" + value.subType() + ")"
    end if

    return "<" + valueType + ">"
end function

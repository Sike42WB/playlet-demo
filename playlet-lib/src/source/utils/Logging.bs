import "pkg:/source/roku_modules/log/LogMixin.brs"

enum LogLevel
    ERROR = 0
    WARN = 1
    INFO = 2
    VERBOSE = 3
    DEBUG = 3
end enum

function InitializeLogging() as void
    instance = log.getLogInstance()
    if instance <> invalid
        return
    end if
    log.initializeLogManager(["log_PrintTransport", "log_FileTransport"], LogLevel.INFO)
end function

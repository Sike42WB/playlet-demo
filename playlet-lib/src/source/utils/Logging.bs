enum LogLevel
    ERROR = 0
    WARN = 1
    INFO = 2
    DEBUG = 3
end enum

function InitializeLogger(logLevel = LogLevel.Info as LogLevel) as void
    logger = CreateObject("roSGNode", "Logger")
    logger.logLevel = logLevel
    m.global.addFields({ logger: logger })
    logger.control = "run"
    ' params:
    '   - logFile: string
end function


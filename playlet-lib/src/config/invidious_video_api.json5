{
  endpoints: [
    {
      name: "trending",
      displayName: "Trending",
      url: "/api/v1/trending",
      queryParams: {
        type: {
          required: false,
          type: ["Music", "Gaming", "Movies"],
        },
        region: {
          required: false,
          type: "#ISO3166",
          default: "GetUserCountryCode",
        },
        fields: {
          required: false,
          type: "string",
          default: "title,videoId,videoThumbnails,lengthSeconds,viewCount,author,publishedText,liveNow,isUpcoming,premiereTimestamp",
        },
      },
    },
    {
      name: "popular",
      displayName: "Popular",
      url: "/api/v1/popular",
      queryParams: {
        fields: {
          required: false,
          type: "string",
          default: "title,videoId,videoThumbnails,lengthSeconds,viewCount,author,publishedText,liveNow,isUpcoming,premiereTimestamp",
        },
      },
    },
    {
      name: "search",
      displayName: "Search",
      url: "/api/v1/search",
      queryParams: {
        q: {
          required: true,
          type: "string",
        },
        sort_by: {
          required: false,
          type: ["relevance", "rating", "upload_date", "view_count"],
        },
        date: {
          required: false,
          type: ["hour", "today", "week", "month", "year"],
        },
        duration: {
          required: false,
          type: ["short", "long"],
        },
        type: {
          required: false,
          type: ["video", "playlist", "channel", "all"],
        },
        features: {
          required: false,
          type: [
            "hd",
            "subtitles",
            "creative_commons",
            "3d",
            "live",
            "purchased",
            "4k",
            "360",
            "location",
            "hdr",
          ],
        },
        region: {
          required: false,
          type: "#ISO3166",
          default: "GetUserCountryCode",
        },
        fields: {
          required: false,
          type: "string",
          default: "title,videoId,videoThumbnails,lengthSeconds,viewCount,author,publishedText,liveNow,isUpcoming,premiereTimestamp",
        },
      },
    },
    {
      name: "auth_feed",
      displayName: "User Feed",
      url: "/api/v1/auth/feed",
      authenticated: true,
      responseHandler: "AuthFeedHandler",
    },
    {
      name: "playlists",
      displayName: "Playlists",
      url: "/api/v1/auth/playlists",
      authenticated: true,
    },
    {
      name: "playlist",
      displayName: "Playlist",
      url: "/api/v1/playlists/:plid",
      responseHandler: "PlaylistHandler",
    },
    {
      name: "mix",
      displayName: "Mix",
      url: "/api/v1/mixes/:rdid",
      responseHandler: "PlaylistHandler",
    },
    {
      name: "channel",
      displayName: "Channel",
      url: "/api/v1/channels/videos/:ucid",
      queryParams: {
        sort_by: {
          required: false,
          type: ["newest", "oldest", "popular"],
        },
        fields: {
          required: false,
          type: "string",
          default: "title,videoId,videoThumbnails,lengthSeconds,viewCount,author,publishedText,liveNow,isUpcoming,premiereTimestamp",
        },
      },
    },
  ],
}

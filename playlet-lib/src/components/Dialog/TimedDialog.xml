<component name="TimedDialog" extends="StandardMessageDialog">
    <interface>
        <field id="actualButtons" type="stringarray" />
        <field id="defaultButtonIndex" type="integer" value="-1" />
        <field id="defaultActionTimeoutSeconds" type="integer" value="10" />
        <!-- Use actualButtonSelected because buttonSelected is read only and defaults to 0 -->
        <field id="actualButtonSelected" type="integer" value="-1" />
        <field id="startCountdown" type="boolean" alwaysNotify="true" onChange="OnStartCountdown" />
    </interface>
</component>
<?xml version="1.0" encoding="UTF-8"?>

<component name="WebAppScreen" extends="Group">
    <interface>
        <field id="url" type="string" onChange="OnUrlSet" />
    </interface>
    <children>
        <Label
            height='60'
            width='1280'
            font='font:MediumBoldSystemFont'
            horizAlign="center"
            translation="[0,92]"
            text="Scan the QR code with your phone camera, or open the URL in a web browser:"
        />
        <QRPoster
            id='SettingsQRPoster'
            loadWidth='400'
            loadHeight='400'
            loadPadding='20'
            translation='[440, 160]' />

        <Label
            id='UrlLabel'
            height='50'
            width='1280'
            font='font:MediumBoldSystemFont'
            horizAlign="center"
            translation="[0,600]"
        />
    </children>
</component>
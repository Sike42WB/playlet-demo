<component name="Profiles" extends="Node" includes="AutoBind">
    <interface>
        <!-- 
            The version of the profiles. This is used to determine if the
            profiles saved need to be migrated. If the version is less than the
            current version, a migration is needed.
         -->
        <field id="__version" type="integer" value="1" />
        <field id="currentProfile" type="node" alwaysNotify="true" onChange="OnCurrentProfile" />
        <field id="content" type="node" />

        <field id="loginWithProfile" type="node" onChange="OnLoginWithProfile" />

        <field id="invidious" type="node" />
    </interface>
    <children>
        <!-- Note: two content nodes because we're rendering inside a RowList -->
        <ContentNode id="ProfilesContent">
            <ContentNode>
                <ProfileContentNode type="login">
                </ProfileContentNode>
            </ContentNode>
        </ContentNode>
    </children>
</component>
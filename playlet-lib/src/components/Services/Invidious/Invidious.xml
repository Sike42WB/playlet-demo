<component name="Invidious" extends="Node">
    <interface>
        <field id="applicationInfo" type="node" />
        <field id="preferences" type="node" />
        <field id="webServer" type="node" />

        <field id="authToken" type="assocarray" onChange="OnAuthTokenChange" />
        <field id="apiDefinitions" type="assocarray" />
        <field id="refreshSubscribtions" type="boolean" alwaysNotify="true" onChange="GetSubscriptions" />
        <field id="subscriptions" type="assocarray" />

        <function name="GetCurrentInstance" />
        <function name="GetAuthorizeTokenLink" />
        <function name="SetAuthToken" />
    </interface>
</component>
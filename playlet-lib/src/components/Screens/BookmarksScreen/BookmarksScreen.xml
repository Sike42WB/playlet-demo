<?xml version="1.0" encoding="UTF-8"?>

<component name="BookmarksScreen" extends="Group" includes="AutoBind">
    <interface>
        <field id="applicationInfo" type="node" bind="/ApplicationInfo" />
    </interface>
    <children>
        <Group id="noBookmarks" visible="false">
            <Label
                height='720'
                width='1280'
                font='font:MediumBoldSystemFont'
                horizAlign="center"
                vertAlign="center"
                text="You currently have no bookmarks.&#xA;&#xA;To add bookmarks, long-press 'OK' on a video, playlist or channel."
                wrap="true" />
        </Group>
    </children>
</component>
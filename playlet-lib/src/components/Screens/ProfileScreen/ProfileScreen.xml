<component name="ProfileScreen" extends="Group" includes="AutoBind,Focus">
    <interface>
        <field id="navBar" type="node" bind="/NavBar" />
    </interface>

    <children>
        <ProfileRowList
            id="ProfileRowList"
            translation="[0,160]"
            numRows="2"
            rowitemSize="[[240,340]]"
            rowItemSpacing="[[0,0]]"
            itemSize="[1280,500]"
            focusXOffset="[200]"
            itemComponentName="ProfileRowListCell"
            rowFocusAnimationStyle="floatingfocus"
            focusBitmapUri="pkg:/images/focus-glow-padding-19.9.png">
            <ContentNode role="content">
                <ContentNode>
                    <ProfileContentNode type="login">
                    </ProfileContentNode>
                </ContentNode>
            </ContentNode>
        </ProfileRowList>
    </children>
</component>
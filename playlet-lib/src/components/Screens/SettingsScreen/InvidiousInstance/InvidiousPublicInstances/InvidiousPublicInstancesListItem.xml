<component name="InvidiousPublicInstancesListItem" extends="Group">
    <interface>
        <field id="itemContent" type="node" onChange="OnContentSet" />

        <field id="name" type="string" alias="nameLabel.text" />
        <field id="version" type="string" alias="versionLabel.text" />
        <field id="users" type="string" alias="usersLabel.text" />
        <field id="signup" type="string" alias="signupLabel.text" />
        <field id="location" type="string" alias="locationLabel.text" />
        <field id="flag" type="string" alias="flagEmoji.uri" />
        <field id="health" type="string" alias="healthLabel.text" />

        <field id="nameLabelWidth" type="float" alias="nameLabel.width" />
        <field id="versionLabelWidth" type="float" alias="versionLabel.width" />
        <field id="usersLabelWidth" type="float" alias="usersLabel.width" />
        <field id="signupLabelWidth" type="float" alias="signupLabel.width" />
        <field id="locationLabelWidth" type="float" alias="locationLabel.width" />
        <field id="healthLabelWidth" type="float" alias="healthLabel.width" />
    </interface>
    <children>
        <LayoutGroup layoutDirection="horiz">
            <Label id="nameLabel" font="font:SmallestSystemFont" />
            <Label id="versionLabel" font="font:SmallestSystemFont" />
            <Label id="usersLabel" font="font:SmallestSystemFont" />
            <Label id="signupLabel" font="font:SmallestSystemFont" />
            <Label id="locationLabel" font="font:SmallestSystemFont">
                <Poster id="flagEmoji" width="16" height="16" />
            </Label>
            <Label id="healthLabel" font="font:SmallestSystemFont" />
        </LayoutGroup>
    </children>
</component>
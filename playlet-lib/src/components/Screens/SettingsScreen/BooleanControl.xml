<?xml version="1.0" encoding="UTF-8"?>

<component name="BooleanControl" extends="LayoutGroup" includes="Focus">
    <interface>
        <field id="displayText" type="string" alias="Checkbox.title" />
        <field id="description" type="string" alias="Description.text" />
        <field id="value" type="boolean" alias="Checkbox.value" onChange="OnValueChange" />
        <function name="BindPreference" />
    </interface>
    <children>
        <Checkbox id="Checkbox" wrapDividerHeight="0" wrapDividerBitmapUri="" />
        <Label id="Description"
            width="450"
            color="0xb4b4b4ff"
            wrap="true">
            <Font role="font" uri="font:SystemFontFile" size="18" />
        </Label>
    </children>
</component>
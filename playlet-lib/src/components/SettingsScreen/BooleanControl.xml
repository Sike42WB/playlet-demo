<?xml version="1.0" encoding="UTF-8"?>

<component name="BooleanControl" extends="Group">
    <interface>
        <field id="key" type="string" />
        <field id="level" type="int" />
        <field id="value" type="bool" alwaysNotify="true" />
    </interface>
    <children>
        <RadioButtonList id="RadioButtonList">
            <ContentNode role="content">
                <ContentNode title="Yes" />
                <ContentNode title="No" />
            </ContentNode>
        </RadioButtonList>
    </children>
</component>
<?xml version="1.0" encoding="UTF-8"?>

<component name="StringControl" extends="Group">
    <interface>
        <field id="key" type="string" />
        <field id="level" type="int" />
        <field id="value" type="string" alias="TextEditBox.text" />
        <field id="displayText" type="string" />
    </interface>
    <children>
        <TextEditBox id="TextEditBox" clearOnDownKey="false">
        </TextEditBox>
    </children>
</component>
<?xml version="1.0" encoding="UTF-8"?>

<component name="SettingsScreen" extends="Group">
    <interface>
        <field id="appVersion" type="string" value="N/A" onChange="SetVersionLabel" />
        <field id="libVersion" type="string" value="N/A" onChange="SetVersionLabel" />
        <field id="latestVersion" type="string" value="N/A" onChange="SetVersionLabel" />
        <field id="libUrl" type="string" value="N/A" onChange="SetPlayletLibUrlLabel" />
        <field id="url" type="string" onChange="OnUrlSet" />
    </interface>
    <children>
        <Label
            id='VersionLabel'
            height='60'
            width='1280'
            font='font:MediumBoldSystemFont'
            horizAlign="center"
            translation="[0,80]"
        />
        <Label
            id='PlayletLibUrlLabel'
            height='60'
            width='1280'
            font='font:SmallestBoldSystemFont'
            horizAlign="center"
            translation="[0,120]"
        />
        <QRPoster
            id='SettingsQRPoster'
            loadWidth='400'
            loadHeight='400'
            loadPadding='20'
            translation='[440, 160]' />

        <Label
            id='UrlLabel'
            height='50'
            width='1280'
            font='font:MediumBoldSystemFont'
            horizAlign="center"
            translation="[0,600]"
        />
    </children>
</component>
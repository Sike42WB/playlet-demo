<?xml version="1.0" encoding="UTF-8"?>

<component name="SettingsScreen" extends="Group">
    <interface>
        <field id="escape" type="string" alwaysNotify="true" />
        <field id="focus" type="boolean" alwaysNotify="true" />
        <function name="OnkeyEvent" />
        <function name="MenuNavigate" />
        <function name="GetPreferenceValue" />
        <function name="SetPreferenceValue" />
        <function name="GetUserPreferences" />
        <function name="ApplyUserPreferences" />
        <function name="ClearUserPreferences" />
    </interface>
    <children>
        <LayoutGroup
            id="PreferencesMenu"
            layoutDirection="horiz"
            translation="[130,100]"
            itemSpacings="[20]"
        >
        </LayoutGroup>
        <Label
            id="HintLabel"
            height='100'
            width='1020'
            wrap='true'
            font='font:SmallestBoldSystemFont'
            translation="[130,600]"
        />
    </children>
</component>
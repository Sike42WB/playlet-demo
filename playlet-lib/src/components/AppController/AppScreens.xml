<component name="AppScreens" extends="Group" includes="Focus">
    <interface>
        <field id="animateToPosition" type="float" onChange="OnAnimateToPosition" />
    </interface>
    <children>
        <Animation
            id="scrollAnimation"
            duration="0.2">
            <Vector2DFieldInterpolator
                id="scrollAnimationInterpolator"
                key="[ 0.0, 0.5, 1.0 ]"
                fieldToInterp="AppScreens.translation" />
        </Animation>
    </children>
</component>
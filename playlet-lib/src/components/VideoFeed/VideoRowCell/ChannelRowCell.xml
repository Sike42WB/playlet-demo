<component name="ChannelRowCell" extends="Group">
    <interface>
        <field id="itemContent" type="node" onChange="OnContentSet" />
        <field id="type" type="string" value="channel" />
    </interface>
    <children>
        <LayoutGroup
            itemSpacings="[24,5,5]">

            <CirclePoster
                id="thumbnail"
                width="196"
                height="196" />

            <Label
                id="authorLabel"
                width="196"
                font="font:SmallestBoldSystemFont"
                maxLines="2"
                wrap="true" />
            <Label
                id="subCountLabel"
                width="196"
                height="25"
                font="font:SmallestSystemFont" />
            <Label
                id="videoCountLabel"
                width="196"
                height="25"
                font="font:SmallestSystemFont" />
        </LayoutGroup>
    </children>
</component>
<?xml version="1.0" encoding="UTF-8"?>

<component name="ChannelRowCell" extends="Group">
    <interface>
        <field id="itemContent" type="node" onChange="OnContentSet" />
        <field id="type" type="string" value="channel" />
    </interface>
    <children>
        <LayoutGroup
            itemSpacings="[24,5,5]">
            <Poster
                id="thumbnail"
                loadDisplayMode="scaleToZoom"
                width="200"
                height="196">
            </Poster>

            <Label
                id="authorLabel"
                width="200"
                font="font:SmallestBoldSystemFont"
                maxLines="2"
                wrap="true" />
            <Label
                id="subCountLabel"
                width="200"
                height="25"
                font="font:SmallestSystemFont" />
            <Label
                id="videoCountLabel"
                width="200"
                height="25"
                font="font:SmallestSystemFont" />
        </LayoutGroup>
    </children>
</component>
<?xml version="1.0" encoding="UTF-8"?>

<component name="ChannelRowCell" extends="Group">
    <interface>
        <field id="itemContent" type="node" onChange="OnContentSet" />
        <field id="type" type="string" value="channel" />
    </interface>
    <children>
        <LayoutGroup
            itemSpacings="[24,5,5]">
            <MaskGroup
                masksize="[294,294]"
                maskuri="pkg:/images/circle-mask-500.png">
                <Poster
                    id="thumbnail"
                    width="196"
                    height="196" />
            </MaskGroup>

            <Label
                id="authorLabel"
                width="196"
                font="font:SmallestBoldSystemFont"
                maxLines="2"
                wrap="true" />
            <Label
                id="subCountLabel"
                width="196"
                height="25"
                font="font:SmallestSystemFont" />
            <Label
                id="videoCountLabel"
                width="196"
                height="25"
                font="font:SmallestSystemFont" />
        </LayoutGroup>
    </children>
</component>
<?xml version="1.0" encoding="UTF-8"?>

<component name="NavBar" extends="Group" includes="AutoBind">
    <interface>
        <field id="appController" type="node" />
        <field id="buttonGroup" type="node" bind="./NavBarButtonGroup" />
        <field id="items" type="node" />
        <field id="focusIndex" type="int" alwaysNotify="true" onChange="OnFocusIndexChange" />
    </interface>
    <children>
        <Rectangle
            height="720"
            width="115"
            color="#242424"
            opacity="0.9">
            <LayoutGroup
                id="NavBarButtonGroup"
                itemSpacings="[2]"
                translation="[7, 120]">
            </LayoutGroup>
        </Rectangle>
    </children>
</component>
{
  "name": "youtube-roku",
  "version": "0.1.0",
  "description": "Unofficial Youtube client for Roku",
  "dependencies": {
    "bslib": "npm:@rokucommunity/bslib@^0.1.1",
    "log": "npm:roku-log@^0.8.0",
    "maestro": "npm:maestro-roku@^0.47.0",
    "roku-requests": "^0.2.0"
  },
  "devDependencies": {
    "@rokucommunity/bslint": "^0.7.5",
    "brighterscript": "^0.60.4",
    "maestro-roku-bsc-plugin": "^0.25.7",
    "roku-log-bsc-plugin": "^0.6.1",
    "rooibos-roku": "^5.2.3",
    "ropm": "^0.10.10",
    "semver": "^7.3.8"
  },
  "ropm": {
    "rootDir": "src",
    "packageRootDir": "dist",
    "noprefix": [
      "maestro",
      "roku-requests"
    ]
  },
  "scripts": {
    "build": "npm run manifest-version && node scripts/run-roku-youtube.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "manifest-version": "node scripts/update-manifest-version.js",
    "postinstall": "npx ropm copy",
    "lint": "npx bsc"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}

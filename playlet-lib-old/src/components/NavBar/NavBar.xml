<?xml version="1.0" encoding="UTF-8"?>

<component name="NavBar" extends="Group">
    <interface>
        <field id="focusIndex" type="int" alwaysNotify="true" />
        <field id="escapeIndex" type="int" alwaysNotify="true" />
    </interface>
    <children>
        <Rectangle
            height="720"
            width="90"
            color="#242424"
            opacity="0.9">
            <ButtonGroup
                id="NavBarButtonGroup"
                itemSpacings="[2]"
                translation="[7, 120]">
                <IconButton
                    id="searchButton"
                    background="#242424"
                    focusBackground="#ff1c30"
                    padding="20"
                    icon="pkg:/images/icons/search.png"
                    text="Search"
                    height="50"
                    width="90" />
                <IconButton
                    id="homeButton"
                    background="#242424"
                    focusBackground="#ff1c30"
                    padding="20"
                    icon="pkg:/images/icons/home.png"
                    text="Home"
                    height="50"
                    width="90" />
                <IconButton
                    id="settingsButton"
                    background="#242424"
                    focusBackground="#ff1c30"
                    padding="20"
                    icon="pkg:/images/icons/settings.png"
                    text="Settings"
                    height="50"
                    width="90" />
                <IconButton
                    id="webAppButton"
                    background="#242424"
                    focusBackground="#ff1c30"
                    padding="20"
                    icon="pkg:/images/icons/phone.png"
                    text="Web app"
                    height="50"
                    width="90" />
                <IconButton
                    id="appInfoButton"
                    background="#242424"
                    focusBackground="#ff1c30"
                    padding="20"
                    icon="pkg:/images/icons/info.png"
                    text="Info"
                    height="50"
                    width="90" />
            </ButtonGroup>
        </Rectangle>
    </children>
</component>